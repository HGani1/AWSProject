    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "lint:watch": "esw -w --fix ./server/**/*.js app.js",
    "test": "npx mocha --require ts-node/register ./Practice/sinonPractice.ts",
    "watch": "npm run test:watch & npm run lint:watch"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
